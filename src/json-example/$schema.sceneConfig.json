{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$ref": "#/definitions/SceneConfig",
  "definitions": {
    "SceneConfig": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string",
          "title": "场景名"
        },
        "responseDelay": {
          "type": "integer",
          "title": "设置响应延迟(s)"
        },
        "responseCode": {
          "type": "integer",
          "title": "响应状态码"
        },
        "rewriteResponseHeaders": {
          "$ref": "#/definitions/RewriteResponseHeaders"
        },
        "responseData": {
          "$ref": "#/definitions/ResponseData"
        }
      },
      "required": [
        "name",
        "responseCode",
        "responseData",
        "responseDelay",
        "rewriteResponseHeaders"
      ],
      "title": "场景"
    },
    "ResponseData": {
      "type": "object",
      "title": "响应数据"
    },
    "RewriteResponseHeaders": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "cache-control": {
          "type": "string"
        }
      },
      "required": [
        "cache-control"
      ],
      "title": "覆盖响应头"
    }
  }
}