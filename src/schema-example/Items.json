{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "type": "array",
    "items": {
        "anyOf": [
            {
                "$ref": "#/definitions/Item"
            },
            {
                "type": "null"
            }
        ]
    },
    "definitions": {
        "Item": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "id": {
                    "type": "integer"
                },
                "animationId": {
                    "type": "integer"
                },
                "consumable": {
                    "type": "boolean"
                },
                "damage": {
                    "$ref": "#/definitions/Damage"
                },
                "description": {
                    "type": "string"
                },
                "effects": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/Effect"
                    }
                },
                "hitType": {
                    "type": "integer"
                },
                "iconIndex": {
                    "type": "integer"
                },
                "itypeId": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "note": {
                    "type": "string"
                },
                "occasion": {
                    "type": "integer"
                },
                "price": {
                    "type": "integer"
                },
                "repeats": {
                    "type": "integer"
                },
                "scope": {
                    "type": "integer"
                },
                "speed": {
                    "type": "integer"
                },
                "successRate": {
                    "type": "integer"
                },
                "tpGain": {
                    "type": "integer"
                }
            },
            "required": [
                "animationId",
                "consumable",
                "damage",
                "description",
                "effects",
                "hitType",
                "iconIndex",
                "id",
                "itypeId",
                "name",
                "note",
                "occasion",
                "price",
                "repeats",
                "scope",
                "speed",
                "successRate",
                "tpGain"
            ],
            "title": "Item"
        },
        "Damage": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "critical": {
                    "type": "boolean"
                },
                "elementId": {
                    "type": "integer"
                },
                "formula": {
                    "type": "string"
                },
                "type": {
                    "type": "integer"
                },
                "variance": {
                    "type": "integer"
                }
            },
            "required": [
                "critical",
                "elementId",
                "formula",
                "type",
                "variance"
            ],
            "title": "Damage"
        },
        "Effect": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "code": {
                    "type": "integer"
                },
                "dataId": {
                    "type": "integer"
                },
                "value1": {
                    "type": "number"
                },
                "value2": {
                    "type": "integer"
                }
            },
            "required": [
                "code",
                "dataId",
                "value1",
                "value2"
            ],
            "title": "Effect"
        }
    }
}